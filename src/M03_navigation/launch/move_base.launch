<?xml version="1.0"?>
<launch>
  <!-- Arguments -->

  <arg name="cmd_vel_topic" default="/navigation_set_velocity" />
  <!--arg name="cmd_vel_topic" default="cmd_vel" /-->
  <arg name="odom_topic" default="/odom" />
  
  <!-- move_base -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen"  required="true">

    <rosparam file="$(find M03_navigation)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />

    <rosparam file="$(find M03_navigation)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />

    <rosparam file="$(find M03_navigation)/param/local_costmap_params.yaml" command="load" />

    <rosparam file="$(find M03_navigation)/param/global_costmap_params.yaml" command="load" />


    <rosparam file="$(find M03_navigation)/param/move_base_params.yaml" command="load" />

    <!--rosparam file="$(find M03_navigation)/param/base_local_planner_params.yaml" command="load" /-->
    <rosparam file="$(find M03_navigation)/param/dwa_local_planner_params.yaml" command="load" />


    <rosparam file="$(find M03_navigation)/param/global_planner_params.yaml" command="load" />



    <remap from="cmd_vel" to="$(arg cmd_vel_topic)"/>

    <remap from="odom" to="$(arg odom_topic)"/>

  </node>
</launch>
